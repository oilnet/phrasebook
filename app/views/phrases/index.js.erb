<% if @phrases.any? %>
  $('#phrases_list').html('<%= escape_javascript(render @phrases) %>');
<% else %>
  $('#phrases_list').html('<%= t('.no_search_results', default: 'Unfortunately we could not find any phrases for this search.') %>');
<% end %>
$('#logo').css('animation', '');
$('html').css('cursor', 'auto');
